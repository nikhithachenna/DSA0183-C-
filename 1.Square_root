#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    int num;
    float result;
    cout<<"Enter a number ";
    cin>>num;
    if(num==0||num==1)
    {
        cout<<"the squareroot of the number is "<<num;
    }
    else
    {
        result=pow(num,0.5);
        cout<<"the squareroot of the given number is "<<result;
    }
    return 0;
}
